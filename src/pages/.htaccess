RewriteEngine On

# Удаление расширения .html
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html [L]

# Удаление src/pages/main/ из URL
RewriteCond %{REQUEST_URI} !^/src/pages/main/
RewriteRule ^about$ /src/pages/main/about.html [L]

# Пример правила для других страниц
RewriteRule ^(.*)$ /src/pages/main/$1.html [L]